generator client {
  provider = "prisma-client-js"
  output   = "./gameDataClient"
}

datasource db {
  provider = "mysql"
  url      = env("ITEM_DATABASE_URL")
}

model Item {
  id     Int    @id @default(autoincrement()) @map("item_id")
  name   String @unique @map("name")
  health Int?   @map("health")
  power  Int?   @map("power")
  price  Int    @map("price")

  @@map("item")
}
